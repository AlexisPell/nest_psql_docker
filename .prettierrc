{
    "disableLanguages": [],
  "useTabs": true,
  "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "arrowParens": "always",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": ["*.ts"],
            "options": {
                "parser": "typescript"
            }
        },
        {
            "files": ["*.json"],
            "options": {
                "parser": "json"
            }
        }
    ]
}